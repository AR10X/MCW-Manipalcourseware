<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <!--meta tag for responsive viewport-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <title>ManipalCourseWare</title>

  <!-- google fonts link below here-->
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  <link href="https://fonts.googleapis.com/css?family=Raleway:200,100,400" rel="stylesheet" type="text/css" />

  <!--Bootstrap 4 CSS-->
  <link rel="stylesheet" href="css/bootstrap.min.css">
  <!-- Our Custom CSS -->
  <link rel="stylesheet" href="css/style.css" >
  <!-- <link rel="stylesheet" href="css/index.css" > -->
  <link rel="stylesheet" href="css/mouseScroll.css" >
  <link rel="stylesheet" href="css/spinner.css" >
  <link rel="stylesheet" href="css/scrollbar.css" >
  <link rel="stylesheet" href="css/videoConsole.css" >
  <!-- <link rel="stylesheet" href="css/videoPage.css" > -->
 
  <!--fontawesome icons kitcode below-->
  <script src="https://kit.fontawesome.com/f18f73cc60.js" crossorigin="anonymous"></script>

  <!--Jquery&popperjs link below-->
  <script type="text/javascript" src="js/jquery-3.5.1.min.js"></script>
  <script type="text/javascript" src="https://unpkg.com/@popperjs/core@2"></script>
  <script type="text/javascript" src="js/bootstrap.min.js"></script>
  <script type="text/javascript" src="js/functionality.js"></script>
</head>
<body >

<!--CSS Spinner-->
<div class="spinner-wrapper">
    <div class="spinner"></div>
</div>

  <div class="wrapper">
    <!-- Sidebar Holder -->
    <nav id="sidebar" class="sticky-top active">
        <div class="sidebar-logo">
            <img class="img-fluid mx-auto d-block" src="img/logo-icon.png">
        </div>
        <ul class="list-unstyled components">
            <li class="active">
                <a href="index.html"><i class="fas fa-home"></i>  HOME</a>
            </li>
            <li>
                <a href="/explore"><i class="fas fa-play-circle"></i>  EXPLORE</a>
            </li>
            <li>
                <p>FOLLOW US</p>
            </li>
            <div class="socialmedia-icons">
                <a href="#"><i class="fab fa-instagram"></i></a>
                <a href="#"><i class="fab fa-facebook-square"></i></a>
            </div>
        </ul>
        <div class="sidebar-footer">
            <ul class="list-unstyled">
                <li><i class="fa fa-copyright" aria-hidden="true"></i>Copyright 2020</li>
                <li>Made with <span><i class="fa fa-heart" aria-hidden="true"></i></span> by Abdul Rehaman</li>
            </ul>
        </div>
    </nav>

    <!-- Page Content Holder -->
    
    <div id="content">
        <nav class="navbar sticky-top navbar-expand-sm topbar-custom-video position-fixed top-0 w-100 px-0">
            <div class="container-fluid justify-content-start ">
                <button type="button" id="sidebarCollapse" class="navbar-btn active">
                    <span></span>
                    <span></span>
                    <span></span>
                </button>
                <a class="navbar-brand mr-auto" href="index.html">ManipalCourseWare</a>
            </div>
        </nav>
        <div class="container-fluid main-section">
            <div class="row">
                <div class="col-lg-5 order-2 order-lg-1 left ">
                    <p>MME/A/KOM</p>
                    <% print.forEach((lecture)=>{ %>
                        <form action="/video-page/<%=lecture.link%>" method="POST">
                            <button type="submit" class="btn-card">
                                <div class="video-card">
                                    <div class="row">
                                        <div class="col-lg-6 video-heading">
                                            <p><%=lecture.lecture_no %> . <%=lecture.lecture_topic %></p><br>
                                            <p><%=lecture.date_time %><br><%=lecture.teacher_name %></p>
                                        </div>
                                        <div class="col-lg-6 video-img">
                                            <img src="img/thumbnail.png" class="img-thumbnail">
                                        </div>
                                    </div>
                                </div>
                            </button>
                        </form>
                    <% }); %>
                </div>
                <div class="col-lg-7 order-1 order-lg-2 right">
                    <h1>KINEMATICS OF MACHINERY</h1>
                    <a href="/explore" class="btn btn-info mainmenu-btn" role="button">GO BACK TO MAIN MENU</a>
                    <div class="video-player">
                        
                            <div class="video-inside">
                                <iframe width="640" height="360" src="https://web.microsoftstream.com/embed/video/?autoplay=false&showinfo=true" allowfullscreen style="border:none; position: absolute; top: 0; left: 0; right: 0; bottom: 0; height: 100%; max-width: 100%;">
                                </iframe>
                            </div>
                        <!-- <div style='max-width: 640px'>
                            <div style='position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden;'>
                                <iframe width="640" height="360" src="https://web.microsoftstream.com/embed/video/?autoplay=false&showinfo=true" allowfullscreen style="border:none; position: absolute; top: 0; left: 0; right: 0; bottom: 0; height: 100%; max-width: 100%;">
                                </iframe>
                            </div>
                        </div> -->
                    </div>
                      
                </div>
            </div>
        </div>
    </div>
    
    <!-- <div class="footer">
        <ul class="list-unstyled">
            <li><i class="fa fa-copyright" aria-hidden="true"></i>Copyright 2022</li>
            <li>Made with <span><i class="fa fa-heart" aria-hidden="true"></i></span> by Abdul Rehaman</li>
        </ul>
    </div> -->
</div>
</body>
</html>
