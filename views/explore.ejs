  <!-- Page Content Holder -->
  
  <div id="content">
      <nav id="navbar" class="navbar navbar-expand-sm sticky-top topbar-custom ">
          <div class="container-fluid justify-content-start ">
              <button type="button" id="sidebarCollapse" class="navbar-btn active">
                  <span></span>
                  <span></span>
                  <span></span>
              </button>
              <a class="navbar-brand mr-auto" href="index.html">ManipalCourseWare</a>
              <div class="crowdsource ">
                <ul class="nav">
                  <li class="nav-items">
                      <form action="/logout?_method=DELETE" method="POST">
                        <button type="submit" class="btn crowdsource-btn">LOGOUT</button>
                      </form>
                  </li>
                </ul>
              </div>
          </div>
