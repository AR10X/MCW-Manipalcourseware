        <!--Explore page-->
          <h1 class="explore-heading">SELECT YOUR CLASS</h1>
          <h3 class="explore-disclaimer">Please note that access to the videos on this platform is restricted to the specific branch and section that you are enrolled in.</h3>
            <form action="/videoConsole" method="POST">
              <div id="explore" class="explore-page">
                <div class="row">
                  <div class="col-lg-4 mb-4">
                    <div class="class-option">
                      <div class="circle">
                        <p>1</p>
                      </div>
                      <h3>SELECT BRANCH</h3>
                      <h4>Please seclect your branch. </h4>
                      <div class="form-group">
                        <select class="form-control" id="branch" name="branch" onChange="changecat(this.value);">
                          <option value="" disabled selected>Select</option>
                          <option value="AERO">AERO: AERONAUTICAL</option>
                          <option value="AUTO">AUTO: AUTOMOBILE</option>
                          <option value="BME">BME: BIOMEDICAL</option>
                          <option value="BIO">BIO: BIOTECHNOLOGY</option>
                          <option value="CHE">CHE: CHEMICAL</option>
                          <option value="CIV">CIV: CIVIL</option>
                          <option value="CCE">CCE: COMPUTER & COMMUNICATION</option>
                          <option value="CSE">CSE: COMPUTER SCIENCE</option>
                          <option value="ELE">ELE: ELECTRICAL & ELECTRONICS</option>
                          <option value="ECE">ECE: ELECTRONICS & COMMUNICATION</option>
                          <option value="IP">IP: INDUSTRIAL & PRODUCTION</option>
                          <option value="IT">IT: INFORMATION TECHNOLOGY</option>
                          <option value="EI">E&I: ELECTRONICS & INSTRUMENTATION</option>
                          <option value="MME">MME: MECHANICAL</option>
                          <option value="MTE">MTE: MECHATRONICS</option>
                          <option value="MED">MED: MEDIA TECHNOLOGY</option>
                        </select>
                      </div>
                      <br>
                      <br>
                    </div>
                  </div>
                  <div class="col-lg-4 mb-4">
                    <div class="section-option">
                      <div class="circle">
                        <p>2</p>
                      </div>
                      <h3>SELECT SECTION</h3>
                      <h4>Please seclect your section.</h4>
                      <div class="form-group">
                        <select class="form-control" id="section" name="section" >
                          <option value="" disabled selected>Select</option>
                        </select>
                      </div>
                      <br>
                      <br>
                    </div>
                  </div>
                  <div class="col-lg-4 mb-4">
                    <div class="subject-option">
                      <div class="circle">
                        <p>3</p>
                      </div>
                      <h3>SELECT SUBJECT</h3>
                      <h4>Please seclect the subject.</h4>
                      <div class="form-group">
                        <select class="form-control"  id="subject" name="subject">
                          <option value="" disabled selected>Select</option>
                        </select>
                      </div>
                      <br>
                      <br>
                    </div>
                  </div>
                </div>
              </div>
              <button class="btn btn-info submit">SUBMIT</button>
            </form>
            <div class="cloud">
                <img src="img/cloud2.png">
            </div>
        <div class="footer">
